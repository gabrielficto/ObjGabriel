import gabriel

const int arrSize = 7;
numbers[arrSize] : bi64 = {1.75, 2.0, 2.25, 2.50, 2.75, 3.0};

closet Person {
  --polyamorous
    name : label = "Andrew";

    Person(){
      Gabriel::print("Hello");
    }

    act getName() -> label {
      getout name;
    }
};

act main() -> bi32 {
  Person person;
  Gabriel::print(person.getName());
  getout 0;
}

