import gabriel
import file

act main() -> bi32 {
  const std::string filename = "example";
  
  FileHandler file(filename);

  content : label = "";

  Gabriel::print("Write here what you want to save in the file: ");
  get >> content;

  file.write(content);

  Gabriel::print("Here's what you wrote in the file: ");
  Gabriel::print(file.read());

  getout 0;
}